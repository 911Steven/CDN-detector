            StackExchange.realtime.subscribeToRealtimeQuestions(
                'ws://qa.sockets.stackexchange.com',
                '155-questions-active',
                getPollingUrl,
                10,
                false,
                addQuestion);

            setInterval(updateRelativeTimes, 60000);

            function addQuestion(q) {
                var $question;
                var $existingQuestion;
                var realtimeKey;

                if (!sitesData[q.siteBaseHostAddress]) {
                    return;
                }

                $question = $('#realtime-template .realtime-question').clone();
                realtimeKey = 'realtime-' + sitesData[q.siteBaseHostAddress].hostShortName.replace('.', '') + '-' + q.id;
                $question.attr('class', 'question-container realtime-question ' + realtimeKey);
                $question.attr('data-sid', q.siteBaseHostAddress);
                $question.find('.realtime-question-url').attr('href', q.url);
                $question.find('.realtime-site-icon').attr('src', sitesData[q.siteBaseHostAddress].iconUrl48);
                $question.find('.realtime-title').html(q.titleEncodedFancy);
                $question.find('.realtime-body-summary').html(q.bodySummary);
                $.each(q.tags, function (i, tag) {
                    $question.find('.realtime-tags').append('<a href="http://' + q.siteBaseHostAddress + '/questions/tagged/' + encodeURI(tag) + '" class="post-tag">' + tag + '</a> ');
                });
                $question.find('.realtime-activity-date').text(StackExchange.realtime.getRelativeTime(q.lastActivityDate));
                $question.find('.realtime-unix-time').val(q.lastActivityDate);

                if (q.ownerDisplayName) {
                    if (q.ownerUrl) {
                        $question.find('.realtime-owner').html('<a href="http://stackexchange.com/'&#32;+&#32;q.ownerUrl&#32;+&#32;'">' + q.ownerDisplayName + '</a> on');
                    }
                    else {
                        $question.find('.realtime-owner').text(q.ownerDisplayName + ' on');
                    }
                }
                else {
                    $question.find('.realtime-owner').remove();
                }

                $question.find('.realtime-host').attr('href', 'http://' + q.siteBaseHostAddress).text(sitesData[q.siteBaseHostAddress].hostShortName);
                
                $existingQuestion = $('.' + realtimeKey);

                if ($existingQuestion.length) {
                    $existingQuestion.remove();
                }
                else if ($('.realtime-question:visible').length >= 30) {
                    $('.realtime-question:visible:last').remove();
                }

                updateRelativeTimes();

                $('#mainArea br.cbo').after($question.hide());
                $question.fadeIn();
            }

            function updateRelativeTimes () {
                $('.realtime-unix-time').each(function () {
                    var $this = $(this);
                    var relativeTime = StackExchange.realtime.getRelativeTime(parseInt($this.val(), 10));
                    $this.siblings('.realtime-activity-date').text(relativeTime);
                });
            }

            function getPollingUrl () {
                var $timeElem = $('.realtime-question:visible:first .realtime-unix-time');
                var time = $timeElem.length ? parseInt($timeElem.val(), 10) : Math.floor((new Date().getTime()) / 1000);
                return '/questions/poll-realtime?since=' + time;
            }
        });
        
    </script>

