        <script>
        function rare_stats_reading_now() {
            jQuery.getJSON("//f1m-rare-counter.herokuapp.com/visitors", function(data){
                window.rare._reading_now = data.count; // for debugging
                var elem = jQuery( '#stats-reading-now .stats' );
                jQuery({count: elem.text()}).animate({count: data.count}, {
                    duration: 500,
                    step: function() {
                        elem.text(parseInt(this.count));
                    },
                    done: function(){
                        elem.text(data.count);
                    }
                });
                var threshold = ( jQuery(window).scrollTop() < 50 ) ? 7500 : 20000;
                setTimeout(rare_stats_reading_now, threshold);
            }).error(function(){ jQuery('#stats-reading-now').hide(); });
        }
        setTimeout(rare_stats_reading_now, 200);
        </script>
                
