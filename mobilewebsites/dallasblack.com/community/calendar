								<div class="subscriptionSignup subscriptionForm" id="lauoutSubscriptionForm">

	

		
	 <table>
		<tr>
	
			<td>
				<fieldset class="form-horizontal">
					<div class="control-group">
						<div class="controls" style="margin-left: 0px; float: left;">
							<input name="email" style="border: none; width: 170px; height: 30px; padding: 0 10px" value="" />
						</div>
						<a href="javascript:void(0);" class="btn btn-primary submit">SIGN UP</a>
					</div>
					<div class="options" style="visibility: visible; display: none;">
						<div class="error"></div>
						<div class="checkbox">
							<input name="newsletter" value="daily" checked="checked" type="checkbox" id="shop-the-looks-checkbox">
							<label for="shop-the-looks-checkbox">
								<b>Daily Buzz</b>
								<span>Receive daily updates</span>
							</label>
