<script type="text/javascript">

    $(function () {

        var now = new Date();
        var availableDates = new Array();

        function fillAvailableDates() {
            availableDates = [];
            var dates = JSON.parse($('#availableDatesJson').val());
            for (i = 0; i < dates.length; i++) {
                var datetemp = new Date(Date.parse(dates[i]));
                availableDates[i] = new Date(datetemp.getFullYear(), datetemp.getMonth(), datetemp.getDate(), 0, 0, 0, 0).valueOf();
            }
        }
        fillAvailableDates();

        var allDaysLink = $('<div class="all-days">All Dates</div>');
        
        allDaysLink.click(function () {
            if ($('#selectedDateValue').val() != '') {
                $('#selectedDateValue').val('');
                $('#selectedDateText').text('All Dates');

                if (availableDates.length > 0)
                    $('a#datepicker-toggle').datepicker('setValue', availableDates[0]);
                else
                    $('a#datepicker-toggle').datepicker('setValue', now);

                $('a#datepicker-toggle').data('date', '');
                AcdbCommon.ajaxRequest('/action/Event/GetEventsListChunk', getRequestData(), showResult, false);
            }
        });

        $('a#datepicker-toggle').datepicker({
            format: 'mm-dd-yyyy',
            onRender: function (date) {
                $('.datepicker-days .table-condensed').before(allDaysLink);
            	//return date.valueOf() < now.valueOf() || availableDates.indexOf(new Date(date.getFullYear(), date.getMonth(), date.getDate(), 0, 0, 0, 0).valueOf()) < 0 ? 'disabled' : '';
                return availableDates.indexOf(new Date(date.getFullYear(), date.getMonth(), date.getDate(), 0, 0, 0, 0).valueOf()) < 0 ? 'disabled' : '';
            }
        }).on('changeDate', function (ev) {
            $('#selectedDateValue').val($('a#datepicker-toggle').data('date'));
            $('#selectedDateText').text($('a#datepicker-toggle').data('date'));
            AcdbCommon.ajaxRequest('/action/Event/GetEventsListChunk', getRequestData(), showResult, false);
        });

        function getRequestData() {
            var data = {};
            data["categoryId"] = $('#selectedCategoryValue').val();
            data["dateString"] = $('#selectedDateValue').val();
            return data;
        }
        
        function showResult(result, append) {
        	append == true ? $("#EventsList").append(result) : $("#EventsList").html(result);
        	$(".dataloading").hide();
            if ($("#EventsList").find("#lastElement").length > 0) {
                $("#showMoreBtn").hide();
            }
            else $("#showMoreBtn").show();

            if (append)
                $('input#recordsCount[rel="EventsList"]').val(parseInt($('input#recordsCount[rel="EventsList"]').val()) + 21);
            else
                $('input#recordsCount[rel="EventsList"]').val(21);
        }
        
        if ($("#EventsList").find("#lastElement").length == 0)
            $("#showMoreBtn").show();
        

        $('#showMoreBtn').click(function (e) {
            var recordsCount = $('input#recordsCount[rel="EventsList"]').val();
            var lastDate = $('#EventsList').find('li.list-dates-divider:last').attr('data-date');
            AcdbCommon.ajaxRequest('/action/Event/GetEventsListChunk?skip=' + recordsCount + '&lastDate=' + lastDate, getRequestData(), showResult, true);
        	$(".dataloading").show();
        });

        function updateAvailableDates(result) {
            $('#availableDatesJson').val(result);
            fillAvailableDates();
            if (availableDates.length > 0)
                $('a#datepicker-toggle').datepicker('setValue', availableDates[0]);
            else
                $('a#datepicker-toggle').datepicker('setValue', now);

            $('a#datepicker-toggle').data('date', '');
        }

        $('.categoryFilterLink').click(function (e) {
            $('#selectedCategoryValue').val($(this).attr('data-categoryid'));
            $('#selectedCategoryText').html($(this).find('span:first').html());
            AcdbCommon.ajaxRequest('/action/Event/GetEventsListChunk', getRequestData(), showResult, false);
            AcdbCommon.ajaxRequest('/action/Event/GetCategoryAvailableDates', { categoryId: $('#selectedCategoryValue').val() }, updateAvailableDates, false);
        });                
    });

	window.eventsSubmittingManager = {

		showEventForm: function () {
			var $dialog = $('#sendEventForm').processTemplate().find('.modal');

			var $dialogOverlay = NRecoCommon.showModal($dialog);
			// init
			NRecoCommon.initForm($dialog);
			window.eventsSubmittingManager.initDialogControls($dialog);
			// validation
			NRecoCommon.validateForm($dialog);

			$dialog.find('button.submit,a.submit').click(function () {
				$dialog.find('.error').hide();
				if (NRecoCommon.isValidForm($dialog)) {
					var fields = NRecoCommon.readForm($dialog);
					fields["teaser_image"] = window.eventsSubmittingManager.getFile($dialog.find('.filesList[rel="eventFile"]').first());
					$.ajax({
						type: "POST",
						async: true,
						url: '/action/Event/SubmitEvent',
						data: {
							data: NRecoCommon.jsonSerialize(fields)
						},
						success: function (res) {
							if (Boolean(res.successful)) {
								$dialogOverlay.close();
								var $dialogRes = $('#eventResultForm').processTemplate().find('.modal');
								$dialogRes.find("span#resultHolder").html(res.message);
								var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
							} else {
								NRecoCommon.setStatusText(res.message);
							}
						},
						error: function (XMLHttpRequest, textStatus, errorThrown) {
							$dialog.find('.error').html(textStatus).show('fast');
						}
					});
				}
			});
		},

		initDialogControls: function ($dialog) {

			$('#venue', $dialog).change(function () {
				window.eventsSubmittingManager.prefillAddressFields(this);
			});

			$('#state', $dialog).change(function () {
				$('#state_id').val($(this).find("option:selected").val());
			});

			$dialog.find("#eventFile").change(function () {
				var $fileList = $dialog.find('.filesList[rel="eventFile"]').first();
				$fileList.empty();
				window.eventsSubmittingManager.doEventFileUpload($(this), $fileList);
			});

			var options = {
				'maxCharacterSize': 150,
				'originalStyle': 'originalDisplayInfo',
				'warningStyle': 'warningDisplayInfo',
				'warningNumber': 30,
				'displayFormat': '#left Characters Left'
			};

			$('#details_html', $dialog).textareaCount(options);

			$dialog.find('#state option').filter(function () { return $(this).text() == "Texas" }).attr("selected", "selected");
			$('#state_id').val('#state').find("option:selected").val();

			$("<option>", { text: '', value: '', selected: true }).prependTo("#venue", $dialog);

			$('#startDateControl', $dialog).datetimepicker({
				format: 'yyyy/MM/dd hh:mm',
				language: 'en-US'
			});
		},

		prefillAddressFields: function (venueItm) {
			var venueValue = $(venueItm).find("option:selected").val();
			if (venueValue.length > 0) {
				$.ajax({
					type: "POST",
					async: true,
					url: '/ProviderAjaxHandler.axd',
					data: {
						provider: 'venueByIdPrv',
						context: JSON.stringify({
							'arg': venueValue
						})
					},
					success: function (res) {
						var venueData = NRecoCommon.jsonDeserialize(res);
						$('#address').val(venueData.address);
						$('#city').val(venueData.city);
						$('#zip').val(venueData.zip);
						$('#venue_name').val(venueData.name);

						$('#state option[value="' + venueData.state_id + '"]').attr("selected", "true");
						$('#state_id').val(venueData.state_id);
					}
				});
			} else {
				$('#address').val("");
				$('#city').val("");
				$('#zip').val("");
				$('#state').val("");
				$('#venue_name').val("");
			}
		},

		removeFile: function (elem) {
			$(elem).parent().remove();
		},

		getFile: function (filesListElem) {
			var files = [];
			filesListElem.find("li").each(function () {
				files.push($(this).attr("filepath"));
			});
			return files[0];
		},

		doEventFileUpload: function (inputElem, $filesListElem) {
			var $loading = $('<span class="fileUploading">File upload in progress...</span>');
			$loading.insertAfter(inputElem);
			inputElem.hide();

			$.ajax('/action/Page/UploadEventFile', {
				type: 'POST',
				iframe: true,
				dataType: 'iframe text',
				fileInput: $(inputElem)
			}).success(function (data) {
				var fileDataList = data ? JSON.parse(data) : [];

				if (fileDataList["uploadError"]) {
					var fileLiError = $('<li style="color:red">' + fileDataList["uploadError"] + '</li>');
					$filesListElem.append(fileLiError);
				}
				else {
					$.each(fileDataList, function () {
						var fileData = this;
						var fileLi = $('<li class="file"></li>');
						fileLi.attr("filepath", fileData["filepath"]);


						fileLi.attr("file-id", "");
						var file = $('<a></a>');
						var fileRemove = $('<a><i class="icon-remove-sign"></i></a>');
						file.attr("href", '/' + fileData["url"]);
						file.attr("target", "_blank");
						file.html(fileData["name"]);
						fileLi.append(file);
						fileRemove.attr("href", "javascript:void(0)");
						fileRemove.attr("onclick", "window.eventsSubmittingManager.removeFile(this)");
						fileRemove.addClass("remove");
						fileLi.append(fileRemove);
						filesListElem.append(fileLi);
					});
				}
			}).complete(function () {
				//clear upload
				inputElem.val('');
				$loading.remove();
				inputElem.show();
			});
		}
	};

	$(function () {
		$('#sendEventForm').setTemplate($('#sendEventFormTemplate').html());
		$('#eventResultForm').setTemplate($('#eventResultFormTemplate').html());
	});
</script>
</div>

