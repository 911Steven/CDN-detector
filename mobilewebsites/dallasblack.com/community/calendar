	<script type="text/javascript">
		
		
		
		rightBannersShown = false;

		$('#showMoreNewsBtn').waypoint(function (direction) {
			if (direction == 'down') { $('.rightBannersArea').addClass('fixed'); rightBannersShown = true; }
			if (direction == 'up') { $('.rightBannersArea').removeClass('fixed'); rightBannersShown = false; }
		});

		$(window).scroll(function () {
			var mainBottom = $('.main').offset().top + $('.main').height();
			var bannersBottom = $('.rightBannersArea').offset().top + $('.rightBannersArea').height();
			if ((bannersBottom > mainBottom) && rightBannersShown) {
				$('.rightBannersArea').removeClass('fixed');
			}
			if (($(window).scrollTop() - ($(document).height() - $(window).height()) <= -500) && rightBannersShown) {
				$('.rightBannersArea').addClass('fixed');
			}
		});

		window.NRecoCommon.labels = { "ajaxLoadingMsg": "Loading...", "notEnoughPermissionsMsg": "Not enough permissions", "requiredMsg": "Required field", "invalidEmailMsg": "Invalid e-mail", "tooShortMsg": "Too short", "tooLongMsg": "Too long (max {0} chars)", "invalidDateFormatMsg": "Invalid date format", "invalidIntFormatMsg": "Invalid number", "monthNames": ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"], "dayNamesMin": ["Su", "Mo", "Tu", "We", "Th", "Fr", "Sa"] };

		NRecoCommon.setStatusText = function (s) { window.ajaxLoadingText.startStatus("status", s); };
		NRecoCommon.showModal = function ($dialog) {
			$dialog.modal();
			return {
				close: function () { $dialog.data('modal').hide(); }
			};
		};
		NRecoCommon.valid8Options = {
			validationCssClass: "help-inline",
			parentErrorCssClass: "error",
			parentValidCssClass: "success",
			parentErrorSelector: ".control-group:first"
		};

		jQuery(document).ready(function () {
			try {
				var uids = [];

				jQuery("div.bannerContainer .banner").each(function () {
					var match = $(this).find("div[id^='bannerid']:first").attr("id").match(/bannerid_([^&]+)/i);
					if (match) {
						uids.push(match[1]);
					}
				});

				if (0 < uids.length) {
					$.ajax({
						type: "POST", async: true,
						url: 'http://dallasblack.com/ProviderAjaxHandler.axd',
						data: {
							provider: "updateBannerImpressionOpr",
							context: JSON.stringify({
								'ids': uids.join(',')
							})
						},
						success: function (res) {
						}
					});
				}
			} catch (e) {
				// Ignore any errors
				//alert(e);
			}
		});

		$(document).ready(function (e) {
			copyDiv();
			$('.searchBox #quickSearchTextbox').focus(function () {
				$(this).animate({ width: 250 }, 'fast');

			});

			$('.searchBox #quickSearchTextbox').blur(function (e) {
				$(this).animate({ width: 1 }, 'fast');
			});

			
		});

		$(function () {
			$("#quickSearchTextbox").watermark('');

			if ($('div.contestLeftBlock li').length == 0) {
				$('div.contestLeftBlock').hide();
			}

			$('.collapsible-content-block .content-holder:empty').each(function () { $(this).parents('.collapsible-content-block').first().hide() });

			var doSearch = function () {
				var qText = $('#quickSearchTextbox').val();
				var getResponseForm = $('form#google_searchbox');
				getResponseForm.find('input[name="q"]').val(qText);
				getResponseForm.find('input[name="sa"]').click();
			};
			$('#quickSearchLink').click(function () {
				if ($('#quickSearchTextbox').val().length > 0)
					doSearch();
			});
			$('#quickSearchTextbox').keydown(function (e) {
				if (e.keyCode == 13) {
					if ($('#quickSearchTextbox').val().length > 0)
						doSearch();
					return false;
				}
			});
		});
		function copyDiv() {
			var firstDivContent = document.getElementById('centralCol');
			var secondDivContent = document.getElementById('sideBarBottom');
			var thirdDivContent = document.getElementById('sideBar');
			var winWidth = $(window).width();
			if (winWidth <= 1280) {
				secondDivContent.innerHTML = firstDivContent.innerHTML;
			}
		}
	
		
	</script>

	


