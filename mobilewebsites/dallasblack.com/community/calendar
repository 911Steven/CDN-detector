<script type="text/javascript">
	featuredEventsShown = false;
	$('#featuredEventsList').waypoint(function (direction) {
		if (direction == 'down') { $('.featuredEventsList').addClass('fixed'); featuredEventsShown = true; }
		if (direction == 'up') { $('.featuredEventsList').removeClass('fixed'); featuredEventsShown = false; }
	});

	$(window).scroll(function () {
		var mainBottom = $('.main').offset().top + $('.main').height();
		var featuredBottom = $('.featuredEventsList').offset().top + $('.featuredEventsList').height();
		if ((featuredBottom > mainBottom) && featuredEventsShown) {
			$('.featuredEventsList').removeClass('fixed');
		}
		if (($(window).scrollTop() - ($(document).height() - $(window).height()) <= -500) && featuredEventsShown) {
			$('.featuredEventsList').addClass('fixed');
		}
	});
</script>
</div>

