<script>
    $(function () {
    	$('#inviteForm').setTemplate($('#inviteFormTemplate').html());
    	$('#inviteResultForm').setTemplate($('#inviteResultFormTemplate').html());
    });

    function initEmailsBlock($dialog) {
    	$('#addEmailBtn', $dialog).click(function () {
    		appendEmailBlock();
    	});
    }
    function appendEmailBlock() {
    	var $eTmpl = $('#emailTemplate .g-form').clone();
    	$('.delete-btn', $eTmpl).click(function () {
    		$(this).closest('.g-form').remove();
    	});
    	$('#emailsList').append($eTmpl);
    }

</script>


