<script>

    function appendGuestsBlock() {
        var $gTmpl = $('#guestTempl .g-form').clone();

        $('.delete-btn', $gTmpl).click(function () {
            $(this).closest('.g-form').remove();
        });

        $('#guestsList').prepend($gTmpl);
    }

    function initGuestsBlock($dialog) {      

        $('#addGuestBtn', $dialog).click(function () {
            appendGuestsBlock();
        });
    }

    function getGuestsNames() {

        var fnames = [];
        var lnames = [];
        var names = {};

        $('#guestsList .guest_first_name').each(function () {
            fnames.push($(this).val());
        });

        $('#guestsList .guest_last_name').each(function () {
            lnames.push($(this).val());
        });

        names["first_names"] = fnames;
        names["last_names"] = lnames;

        return names;
    }

</script>
