<script type="text/javascript">

    function removeFile(elem) {
        $(elem).parent().remove();
    };

    function getFiles(filesListElem) {
        var files = [];
        filesListElem.find("li").each(function () {
            files.push({
                Path: $(this).attr("filepath"),
                Id: $(this).attr("file-id")
            }
            );
        });
        return files;
    }

    function doFileUpload(inputElem, filesListElem) {
        var $loading = $('<span class="fileUploading">File upload in progress...</span>');
        $loading.insertAfter(inputElem);
        inputElem.hide();
        
        $.ajax('/action/Page/UploadFeedbackFile', {
            type: 'POST',
            iframe: true,
            dataType: 'iframe text',
            fileInput: $(inputElem)
        }).success(function (data) {
            var fileDataList = data ? JSON.parse(data) : [];
            $.each(fileDataList, function () {
                var fileData = this;
                var fileLi = $('<li class="file"></li>');
                fileLi.attr("filepath", fileData["filepath"]);


                fileLi.attr("file-id", "");
                var file = $('<a></a>');
                var fileRemove = $('<a><i class="icon-remove-sign"></i></a>');
                file.attr("href", '/' + fileData["url"]);
                file.attr("target", "_blank");
                file.html(fileData["name"]);
                fileLi.append(file);
                fileRemove.attr("href", "javascript:void(0)");
                fileRemove.attr("onclick", "removeFile(this)");
                fileRemove.addClass("remove");
                fileLi.append(fileRemove);
                filesListElem.append(fileLi);
            });
        }).complete(function () {
            //clear upload
            inputElem.val('');
            $loading.remove();
            inputElem.show();
        });
}

window.feedbackManager = {

    showFeedbackForm: function (preselectedFeedbackType) {
        var $dialog = $('#feedbackForm').processTemplate().find('.modal');

        var $dialogOverlay = NRecoCommon.showModal($dialog);
        // init
        NRecoCommon.initForm($dialog);
        // validation
        NRecoCommon.validateForm($dialog);

        $('#feedbackType').on('change', function () {
            $dialog.find('div[data-feedback-type]').hide();
            $dialog.find('div[data-feedback-type="' + $(this).val() + '"]').show();
            if ($dialog.find('div[data-feedback-type="' + $(this).val() + '"]').length > 0) {
                $dialog.find('input#feedbackFile').parents('div.control-group:first').show();
                $dialog.find('textarea#message').parents('div.control-group:first').find('.control-label .text').text('Details');
                var subjectFieldCaption = $(this).val() == "business" ? "Business Name" : "Title";
                $dialog.find('input#subject').parents('div.control-group:first').find('.control-label .text').text(subjectFieldCaption);
                if ($(this).val() == "business") {
                    $dialog.find('input#feedbackFile').parents('div.control-group:first').hide();
                }
            } else {
                $dialog.find('input#feedbackFile').parents('div.control-group:first').hide();
                $dialog.find('textarea#message').parents('div.control-group:first').find('.control-label .text').text('Message');
                $dialog.find('input#subject').parents('div.control-group:first').find('.control-label .text').text('Subject');
            }
        });

        if (preselectedFeedbackType) {
            $('#feedbackType').val(preselectedFeedbackType).trigger('change').attr('disabled', 'disabled');
        }

        $dialog.find("#feedbackFile").change(function () {
            doFileUpload($(this), $dialog.find('.filesList[rel="feedbackFile"]').first());
        });

        $dialog.find('button.submit,a.submit').click(function () {
            $dialog.find('.error').hide();
            if (NRecoCommon.isValidForm($dialog)) {
                var fields = NRecoCommon.readForm($dialog);
                fields["files"] = getFiles($dialog.find('.filesList[rel="feedbackFile"]').first());
                $.ajax({
                    type: "POST",
                    async: true,
                    url: '/action/Page/AddFeedback',
					data: {
					    data: NRecoCommon.jsonSerialize(fields)
					},
					success: function (res) {
					    if (Boolean(res.successful)) {
					        $dialogOverlay.close();
					        var $dialogRes = $('#feedbackResultForm').processTemplate().find('.modal');
					        $dialogRes.find("span#resultHolder").html(res.message);
					        var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
					    } else {
					        NRecoCommon.setStatusText(res.message);
					    }
					},
					error: function (XMLHttpRequest, textStatus, errorThrown) {
					    $dialog.find('.error').html(textStatus).show('fast');
					}
				});
            }
		});

	    /*$dialog.find('#send_sms').click(function() {
			$dialog.find('div.mobileNumberHolder').toggle();
		});*/

	}


    };
$(function () {
    $('#feedbackForm').setTemplate($('#feedbackFormTemplate').html());
    $('#feedbackResultForm').setTemplate($('#feedbackResultFormTemplate').html());
});
</script>

