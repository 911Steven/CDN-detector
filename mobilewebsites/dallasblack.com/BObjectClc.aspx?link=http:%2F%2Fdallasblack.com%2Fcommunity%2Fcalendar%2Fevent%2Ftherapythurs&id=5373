<script type="text/javascript">
	featuredEventsShown = false;
	$('#featuredEventsList').waypoint(function (direction) {
		if (direction == 'down') { $('.featuredEventsList').addClass('fixed'); featuredEventsShown = true; }
		if (direction == 'up') { $('.featuredEventsList').removeClass('fixed'); featuredEventsShown = false; }
	});

	$(window).scroll(function () {
		var mainBottom = $('.main').offset().top + $('.main').height();
		var featuredBottom = $('.featuredEventsList').offset().top + $('.featuredEventsList').height();
		if ((featuredBottom > mainBottom) && featuredEventsShown) {
			$('.featuredEventsList').removeClass('fixed');
		}
		if (($(window).scrollTop() - ($(document).height() - $(window).height()) <= -500) && featuredEventsShown) {
			$('.featuredEventsList').addClass('fixed');
		}
	});
</script>
</div>

<script type="text/javascript">

	$(function () {

		$.ajax({
			type: "POST", async: true,
			url: 'http://dallasblack.com/ProviderAjaxHandler.axd',
			data: {
				provider: "updateEventViewsCountOpr",
				context: JSON.stringify({ 'eventAlias': 'therapythurs' })
			},
			success: function (res) {
			}
		});
	});

    function showAddReminderForm() {
        var $dialog = $('#addReminderForm').processTemplate().find('.modal');

        var $dialogOverlay = NRecoCommon.showModal($dialog);
        // init
        NRecoCommon.initForm($dialog);
        // validation
        NRecoCommon.validateForm($dialog);

        $dialog.find('button.submit,a.submit').click(function () {
            $dialog.find('.error').hide();
            if (NRecoCommon.isValidForm($dialog)) {
                var fields = NRecoCommon.readForm($dialog);
                $.ajax({
                    type: "POST",
                    async: true,
                    url: '/action/Event/AddReminder',
                    data: {
                        data: NRecoCommon.jsonSerialize(fields)
                    },
                    success: function (res) {
                        if (Boolean(res.successful)) {
                            $dialogOverlay.close();
                            var $dialogRes = $('#addReminderResultForm').processTemplate().find('.modal');
                            var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
                        } else {
                            NRecoCommon.setStatusText(res.message);
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        $dialog.find('.error').html(XMLHttpRequest.responseText).show('fast');
                    }
                });
            }
        });

        /*$dialog.find('#send_sms').click(function() {
			$dialog.find('div.mobileNumberHolder').toggle();
		});*/

    }

    function showRsvpForm() {
        var $dialog = $('#sendRsvpForm').processTemplate().find('.modal');

        var $dialogOverlay = NRecoCommon.showModal($dialog);
        // init
        NRecoCommon.initForm($dialog);
        initGuestsBlock($dialog);
        // validation
        NRecoCommon.validateForm($dialog);

        $dialog.find('button.submit,a.submit').click(function () {
            var $dialog = $('#sendRsvpForm').find('.modal');
            NRecoCommon.initForm($dialog);
            NRecoCommon.validateForm($dialog);

            $dialog.find('.error').hide();
            if (NRecoCommon.isValidForm($dialog)) {
                var names = getGuestsNames();

                var fields = NRecoCommon.readForm($dialog);
                fields["event_id"] = '9387';
                fields["guests_names"] = names;
                $.ajax({
                    type: "POST",
                    async: true,
                    url: '/action/Event/SendRsvp',
                    data: {
                        data: NRecoCommon.jsonSerialize(fields)
                    },
                    success: function (res) {
                        $dialog.find('.modal-body').html("Your RSVP successfully sent.");
                        $dialog.find('a.btn').hide();
                        $dialog.find('a.close-popup').show();
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        $dialog.find('.error').html(XMLHttpRequest.responseText).show('fast');
                    }
                });
            }
        });
    }

    $(function () {
        $('#addReminderForm').setTemplate($('#addReminderFormTemplate').html());
        $('#addReminderResultForm').setTemplate($('#addReminderResultFormTemplate').html());
        $('#sendRsvpForm').setTemplate($('#sendRsvpFormTemplate').html());
    });
</script>


