    		getLatLngObj: function (lat, lng) {
    			return new google.maps.LatLng(lat, lng);
    		},

    		composeMapOpts : function() {
    			return {
    				zoom: window.gMaps.includeDirections == "true" ? 5 : 11,
    				mapTypeId : google.maps.MapTypeId.ROADMAP,
    				center: this.getLatLngObj(this.lat, this.lng)
    			};
    		},

    		setMarkerByAddress: function (address) {
    			(window.gMaps.geocoder).geocode({ 'address': address }, function (results, status) {
    				if (status == google.maps.GeocoderStatus.OK) {
    					var geocodeResult = results[0].geometry.location;
    					window.gMaps.lat = geocodeResult.lat();
    					window.gMaps.lng = geocodeResult.lng();
    				} else {
    					window.gMaps.lat = '32.7801399';
    					window.gMaps.lng = '-96.80045109999998';

    					$('#GMapBlock').parents('.mapHolder').hide();
    				}
    				window.gMaps.map = new google.maps.Map(jQuery("#map_canvas").get(0), window.gMaps.composeMapOpts());
    				
    				if (window.gMaps.includeDirections != '' && window.gMaps.includeDirections == 'true') {
    					window.gMaps.directionsDisplay = new google.maps.DirectionsRenderer();
    					window.gMaps.directionsDisplay.setMap(window.gMaps.map);
    					window.gMaps.directionsDisplay.setPanel(jQuery('#suggestDirectionsHolder').html('').get(0));
    				} else {
    					var marker = window.gMaps.addMarker();
    				}
    			});
    		},

    		//geocode works async, so it seems that we have only one way to get and add marker: it's create two-way logic. hm...
    		mapInit: function () {
    			if (window.gMaps.lat == "") {
    				window.gMaps.setMarkerByAddress(window.gMaps.address);
    			} else {
    				window.gMaps.map = new google.maps.Map(jQuery("#map_canvas").get(0), window.gMaps.composeMapOpts());
    				if (window.gMaps.includeDirections != '' && window.gMaps.includeDirections == 'true') {
    					window.gMaps.directionsDisplay = new google.maps.DirectionsRenderer();
    					window.gMaps.directionsDisplay.setMap(window.gMaps.map);
    					window.gMaps.directionsDisplay.setPanel(jQuery('#suggestDirectionsHolder').html('').get(0));
    				} else {
    					var marker = window.gMaps.addMarker();
    				}
    			}
    		},

    		addMarker : function () {
    			var marker = new google.maps.Marker({
    				position: this.getLatLngObj(this.lat, this.lng),
    				title: this.name
    			});
    			marker.setMap(this.map);

    			return marker;
    		},

    		loadSuggestionDirectionsBox: function () {
    			var originMarkerPosition = window.gMaps.lat != "" ? window.gMaps.getLatLngObj(window.gMaps.lat, window.gMaps.lng) : $('#toAddress').val();
    			var requestDirection = {
    				origin: originMarkerPosition, 
    				destination: $('#fromAddress').val(),
    				travelMode: google.maps.DirectionsTravelMode.DRIVING
    			};
			
    			this.directionsService.route(requestDirection, function(response, status) {
    				jQuery('#suggestDirectionsHolder').hide();
    				if (status == google.maps.DirectionsStatus.OK) {
    					window.gMaps.directionsDisplay.setDirections(response);
    					jQuery('#suggestDirectionsHolder').show();
    				}
    			});
    		}
    	};

    	$(document).ready(function () {
    		window.gMaps.mapInit();

    		$('#getDirectrionsTrigger').click(function () {
    			window.gMaps.loadSuggestionDirectionsBox();
    		});

    		if (window.gMaps.includeDirections == "true") {
    			window.gMaps.loadSuggestionDirectionsBox();
			}
    	});

    </script>
            </div>

