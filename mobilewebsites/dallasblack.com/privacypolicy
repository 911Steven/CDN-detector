<script type="text/javascript">
		$("#businessLink").click(
            function () {
				$('.small-categorylist li:first-child h4 a').trigger('click');
                if(!$(this).hasClass('expanded')){
                	$("#businessSubMenu").fadeIn(150);
					$(this).addClass('expanded');
				} else {
					$(this).removeClass('expanded');
					$(this).addClass('collapsed');
					$("#businessSubMenu").fadeOut(150);					
				}
            }
        );

</script>







