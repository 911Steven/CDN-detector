<script type="text/javascript">
    $(function () {

        if ($('#preselectedCategory').val() && $('#preselectedCategory').val() != '') {
            $("#selectedCategory").html($('a.categoryFilterLink[data-categoryid="' + $('#preselectedCategory').val() + '"]').text());
            var $cl = $('#cuponsInCategoryLink');
            $cl.attr("href", $('#tmpRouteLink').attr('href') + "/?category=" + $('#preselectedCategory').val());
            $cl.show();
        }

        function showResult(result, append) {
        	var pageSize = 100;
            append == true ? $("#BusinessesList").append(result) : $("#BusinessesList").html(result);
            if ($("#BusinessesList").find("#lastElement").length > 0) {
                $("#showMoreBtn").hide();
            }
            else $("#showMoreBtn").show();

            if (append)
            	$('input#recordsCount[rel="BusinessesList"]').val(parseInt($('input#recordsCount[rel="BusinessesList"]').val()) + pageSize);
            else
            	$('input#recordsCount[rel="BusinessesList"]').val(pageSize);
        }

        if ($("#BusinessesList").find("#lastElement").length == 0)
            $("#showMoreBtn").show();

        $('#showMoreBtn').click(function (e) {
            var recordsCount = $('input#recordsCount[rel="BusinessesList"]').val();
            var data = { keywords: $("#keywords").val(), category: $(categoryHidden).val() };
            AcdbCommon.ajaxRequest('/action/Channel/GetBusinessListChunk?skip=' + recordsCount, data, showResult, true);
        });

        $('.categoryFilterLink').click(function (e) {
            var categoryId = $(this).attr('data-categoryId');
            $('#categoryHidden').val(categoryId);

            var $cl = $('#cuponsInCategoryLink');

            if (categoryId) {
                $cl.attr("href", $('#tmpRouteLink').attr('href') + "/?category=" + categoryId);
                $cl.show();
            }
            else {
                $cl.hide();
            }
            
            var data = { keywords: $("#keywords").val(), category: categoryId };
            var callback = function (result, append) {
                $("#selectedCategory").html($('a.categoryFilterLink[data-categoryid="' + categoryId + '"]').text());
                showResult(result, append);
            };
            AcdbCommon.ajaxRequest('/action/Channel/GetBusinessListChunk', data, callback, false);
        });

    });
</script>

