	<script type="text/javascript">


		window.articleSubscriptionFormMrg = window.articleSubscriptionFormMrg || {};
		window.articleSubscriptionFormMrg = {
			formHolder: '',
			emailInput: '',
			collapsedArea: '',
			errorArea: '',
			newsletterCheckboxes: '',
			validate: function () {
				var hasError, checked$, email;

				articleSubscriptionFormMrg.collapsedArea.show();

				articleSubscriptionFormMrg.errorArea.empty().hide();

				hasError = false;

				checked$ = articleSubscriptionFormMrg.newsletterCheckboxes.filter(":checked");
				if (checked$.size() <= 0) {
					hasError = true;
					articleSubscriptionFormMrg.errorArea.append("<h3><div class='error_img'></div>Please select a newsletter preference.</h3>");
				}

				email = $.trim(articleSubscriptionFormMrg.emailInput.val());
				var pattern = new RegExp(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/);
				if (email === "enter email address" || !pattern.test(email)) {
					hasError = true;
					articleSubscriptionFormMrg.errorArea.append("<h3><div class='error_img'></div>Please enter a valid e-mail address.</h3>");
				}

				if (hasError) {
					articleSubscriptionFormMrg.errorArea.slideDown();
				}
				return !hasError;
			},
			subscribe: function () {

				articleSubscriptionFormMrg.errorArea.hide();
				if (NRecoCommon.isValidForm(articleSubscriptionFormMrg.formHolder) && articleSubscriptionFormMrg.validate()) {
					var subsTypes = [];
					articleSubscriptionFormMrg.newsletterCheckboxes.filter(":checked").each(function () {
						subsTypes.push($(this).val());
					});
					var fields = NRecoCommon.readForm(articleSubscriptionFormMrg.formHolder);
					fields["name"] = fields["email"];
					fields["subscription_types"] = subsTypes;
					$.ajax({
						type: "POST",
						async: true,
						url: '/action/Page/SubscribeNewsletter',
						data: {
							data: NRecoCommon.jsonSerialize(fields)
						},
						success: function (res) {
							var $dialogRes = $('#newsletterResultForm').processTemplate().find('.modal');
							$dialogRes.find("span.resultHolder").html(res.message);
							var $dialogResOverlay = NRecoCommon.showModal($dialogRes);
						},
						error: function (XMLHttpRequest, textStatus, errorThrown) {
							articleSubscriptionFormMrg.errorArea.html(XMLHttpRequest.responseText).show('fast');
						}
					});
				}
			},
			init: function () {

				articleSubscriptionFormMrg.formHolder = $('#' + 'articleSubscriptionForm');
				articleSubscriptionFormMrg.emailInput = articleSubscriptionFormMrg.formHolder.find('input[name="email"]');
				articleSubscriptionFormMrg.collapsedArea = articleSubscriptionFormMrg.formHolder.find('.options');
				articleSubscriptionFormMrg.errorArea = articleSubscriptionFormMrg.formHolder.find('.error');
				articleSubscriptionFormMrg.newsletterCheckboxes = articleSubscriptionFormMrg.formHolder.find("input[name=\"newsletter\"]");
				var $emailInput = this.emailInput;
				var emailEnter = $emailInput.attr("data-val-mouseenter") || "enter email address";
				var emailLeave = $emailInput.attr("data-val-mouseleave") || "NEWSLETTER SIGN UP";
				var emailDefault = emailLeave;

				articleSubscriptionFormMrg.formHolder.on('click', function (e) {
					e.stopPropagation();
				});


				// init
				NRecoCommon.initForm(articleSubscriptionFormMrg.formHolder);
				// validation
				NRecoCommon.validateForm(articleSubscriptionFormMrg.formHolder);

				articleSubscriptionFormMrg.formHolder.find('button.submit,a.submit').click(function () {
					articleSubscriptionFormMrg.subscribe();
				});

				$emailInput
				.val(emailDefault)
				.hover(
					function () {
						var val = $emailInput.val();
						emailDefault = emailEnter;
						if (val === emailLeave) {
							$emailInput.val(emailEnter);
						}
					},
					function () {
						var val = $emailInput.val();
						emailDefault = emailLeave;
						if (val === emailEnter) {
							$emailInput.val(emailLeave);
						}
					}
				)
				.focus(function (e) {
					$emailInput.addClass("focused");
					var val = $emailInput.val();
					if (val === emailEnter || val === emailLeave) {
						$emailInput.val("");
					}

					var $main = $('main');
					var $options = articleSubscriptionFormMrg.formHolder.find('.options');
					var $body = $('body');

					$options.css({
						'visibility': 'hidden',
						'display': 'block'
					});

					$options.addClass('open-upwards');
					$body.one('click', function () {
						$options.slideUp(function () {
							$options.removeClass('open-upwards');
						});
					});

					$options.css({
						'visibility': 'visible',
						'display': 'none'
					});

					$options.slideDown();

				})
				.on('click', function (e) {
					e.stopPropagation();
				})
				.blur(function () {
					$emailInput.removeClass("focused");
					if (!Boolean($.trim($emailInput.val()))) {
						$emailInput.val(emailDefault);
					}
				});
			}
		};

		$(function () {
			articleSubscriptionFormMrg.init();
		});
	</script>
</div>

