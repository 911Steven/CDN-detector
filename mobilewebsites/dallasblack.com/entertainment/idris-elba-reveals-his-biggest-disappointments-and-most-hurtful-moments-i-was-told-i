<script>
    window.SendToFriend = {

        showSendToFriendsForm : function (item_id, source) {
            var $dialog = $('#sendToFriendInviteForm').processTemplate().find('.modal');
			$('div#pbOverlay').css("z-index", "0");

			$dialog.find('input#item_id').val(item_id);
			$dialog.find('input#source').val(source);
            $dialog.find('.modal-footer').find('.btn').show();
            $dialog.find('.modal-footer').find('.processing').hide();
            var $dialogOverlay = NRecoCommon.showModal($dialog);
            // init
            NRecoCommon.initForm($dialog);
            // validation
            var validateEmailsProvided = function(values){
                if (!values.emails || values.emails.length==0)
                    return {valid:false, message:'At least one email must be provided'};

                for(i=0;i<values.emails.length;i++){
                    if (values.emails[i]!='')
                        return {valid:true};
                }

                return {valid:false, message:'At least one email must be provided'};
            }
            var validation = new Object();
            validation.jsFunctions = [
                    { 'function': validateEmailsProvided, values: function() {
                        return { emails: [$dialog.find('#email1').val(),$dialog.find('#email2').val(),$dialog.find('#email3').val(),$dialog.find('#email4').val(),$dialog.find('#email5').val()]}
                    }
                    }
            ];
            var validations = new Array();
            validations["email5"] = validation;
            validations["email4"] = validation;
            validations["email3"] = validation;
            validations["email2"] = validation;
            validations["email1"] = validation;

            NRecoCommon.validateForm($dialog, validations);

            window.AcdbCommon.addRecaptchaWidget('recap');

			$dialog.find('button.close,a.CancelBtn').click(function () {
				$('div#pbOverlay').css("z-index", "9999");
			});

            $dialog.find('button.submit,a.submit').click(function () {
            	$dialog.find('.error').hide();
            	$dialog.find('.captchaErorMsg').hide();

            	if (!grecaptcha.getResponse()) {
            		$dialog.find('.captchaErorMsg').show();
            		return;
            	}

                if (NRecoCommon.isValidForm($dialog)) {
                    var fields = NRecoCommon.readForm($dialog);

                    $dialog.find('.modal-footer').find('.btn').hide();
                    $dialog.find('.modal-footer').find('.processing').show();
                    $.ajax({
                        type: "POST",
                        async: true,
                        url: '/action/Page/SendEmailToFriends',
                        data: {
                            data: NRecoCommon.jsonSerialize(fields)
                        },
                        success: function (res) {
                            if (res.exitCode=='0'){
                                $dialogOverlay.close();
                                SendToFriend.showInviteResultForm(res);
                            }
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            $dialog.find('.error').html("Error on trying to invite friends").show('fast');
                        },
                        complete: function(){
                            $dialog.find('.modal-footer').find('.btn').show();
                            $dialog.find('.modal-footer').find('.processing').hide();
                        }
                    });
                }
            });
        },
        showInviteResultForm: function (resultData) {
            var $dialog = $('#sendResultForm').processTemplate(resultData).find('.modal');

            var $dialogOverlay = NRecoCommon.showModal($dialog);
            // init
            NRecoCommon.initForm($dialog);

			$dialog.find('button.close,a.CancelBtn').click(function () {
				$('div#pbOverlay').css("z-index", "9999");
			});
        }
    }
    $(function () {
        $('#sendToFriendInviteForm').setTemplate($('#sendToFriendInviteFormTemplate').html());
        $('#sendResultForm').setTemplate($('#sendResultFormTemplate').html());
    });
</script>

	
