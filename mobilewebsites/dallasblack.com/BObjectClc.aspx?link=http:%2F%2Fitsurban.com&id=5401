		window.forceLogin = location.hash == "#forcelogin";
		if (window.forceLogin)
			location.hash = "";

		window.fbAsyncInit = function () {
			try {
				FB.init({ appId: '594712940670437', status: true, cookie: true, xfbml: true });
			} catch (e) { }

			$('a.doFacebookLogin').click(function () {
				app.doFacebookLogin();
			});
			$('.signup').fadeIn('slow');

			if (window.forceLogin) {
				setTimeout(function () { app.doFacebookLogin(); }, 500);
			}
		};

		$(function () {

			var e = document.createElement('script');
			e.type = 'text/javascript';
			e.src = document.location.protocol +
				'//connect.facebook.net/en_US/all.js';
			e.async = true;
			document.getElementById('fb-root').appendChild(e);


