  /* Hack for DTI/Hor Market pagination missing classes */
  jQuery(".Pagination ").addClass("row navigation-frame").removeClass("Pagination");
  
  /* write a review pdp fix */
  jQuery('.pdpReviewContentContainer').on ('click', '.pr-write-review-link', function(){
	location.href = jQuery('.write-another-review').attr('href');
  });
  
   /* Temporary fix to change shipping text on Cart, this should be removed when the bug is fixed*/
    jQuery(document).ready(function() {
var match = '3-7 Business Days Shipping:';
        jQuery("table#cartTotalsTable div.ViewSr_shippingSummaryDiv:contains(" + match + "), .ViewSr_shippingSummaryDiv span:contains(" + match + ")").each(function(){
           var $this = $(this);
            $this.html(function(){
                return $this.html().replace(match, "Standard Shipping:");
            });
        });    });
</script>
