<style type="text/css" id="styleSizeFilter">
    div.sizeFilterExpandLink {
        margin: 0px auto;
        text-align: center;
        font-size: 10px;
    }
    div.sizeFilterExpandLink a {
        text-decoration: none;
        color: #0000cc;
    }
    div.sizeFilterExpandLink a:active,
    div.sizeFilterExpandLink a:hover {
        text-decoration: underline;
    }
    div.guidedLeftNavContainer div.sizeFilters ul {
        padding: 0px;
    }
    div.sizeFilters a.showSizesToggle {
        text-align: center;
        margin: 0px auto;
        outline: none;
        padding: 0px;
        width: 100%;
        line-height: 20px;
        clear: both;
        display: block;
    }
    div.guidedLeftNavContainer div.sizeFilters ul li a {
        font-size: 12px;
    }
</style>

<script>
/* Global: jQuery */
jQuery(document).ready(function() {
  	if(jQuery('#myAccountRegisterForm').length){
		jQuery('.register-checkbox-label').text('By placing an order or creating an account, you affirm that you agree to our Terms of Use and Privacy Policy.')
    }

    if (!jQuery('.sizeFilters').length) {
        return;
    }

    var $sizeLIs = jQuery('.sizeFilters li:not(.accordion-navigation)');

    if ($sizeLIs.length < 10) {
        return;
    }

    var maxBoxHeight = jQuery('.sizeFilters ul').height();
    var minBoxHeight = 140;

    jQuery('.sizeFilters ul')
        .css({
            height: minBoxHeight,
            overflow: 'hidden'
        });

    // Rearrange sizes
    var sizeOptionsArray = [];
    $sizeLIs.each(function() {
        sizeOptionsArray.push(jQuery(this)[0].outerHTML);
    });
    var sortedSizeOptionsArray = [];
    var extraSizeOptionsArray = [];
    for (var i = 0; i < sizeOptionsArray.length; i++) {
        if ((sizeOptionsArray[i].toLowerCase().indexOf(">xxs</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">xs</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">s</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">m</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">l</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">xl</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">xxl</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">1x</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">2x</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">3x</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">4x</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">5x</a>") != -1) || (sizeOptionsArray[i].toLowerCase().indexOf(">6x</a>") != -1)) { // These are the ones we want up top
            sortedSizeOptionsArray.push(sizeOptionsArray[i]);
        } else { // These are the extra sizes, leave as-is
            extraSizeOptionsArray.push(sizeOptionsArray[i]);
        }
    }

    // Remove all size links
    var $navUl = jQuery(".sizeFilters ul.nav");
    $navUl.empty();

    // Re-add all size links, with new sort
    for (var i = 0; i < sortedSizeOptionsArray.length; i++) {
        $navUl.append(sortedSizeOptionsArray[i]);
    }

    for (var i = 0; i < extraSizeOptionsArray.length; i++) {
        $navUl.append(extraSizeOptionsArray[i]);
    }

    // Add size toggle link
    var showSizesToggle = '<a href="javascript:" class="showSizesToggle" title="More Sizes">show more sizes >></a>';
    jQuery('.sizeFilters').append(showSizesToggle);

    jQuery('a.showSizesToggle').click(function() {
        if (jQuery('.showSizesToggle').text() === "show more sizes >>") {
            jQuery('.sizeFilters ul').animate({
                height: maxBoxHeight
            }, 200);
            jQuery('.showSizesToggle').text('<< show less sizes');
            return true;
        }
        if (jQuery('.showSizesToggle').text() === "<< show less sizes") {
            jQuery('.sizeFilters ul').animate({
                height: minBoxHeight
            }, 200);
            jQuery('.showSizesToggle').text('show more sizes >>');
            return true;
        }
    });
    var stashHeight;
    jQuery('#Sizes1Title').on('click', function() {
        var $accordion = jQuery('#Sizes1Accordion');
        if ($('#Sizes1-disclosable').hasClass('active')) {
            jQuery('.showSizesToggle').css({
                display: 'none'
            });
            stashHeight = $accordion.css('height');
            $accordion.css('height', 'auto')
        } else {
            jQuery('.showSizesToggle').css({
                display: 'block'
            });
            jQuery('#Sizes1Accordion').css('height', stashHeight)
        }
    });
});</script>
<!-- END: Temporary Override for Left Nav Size Box display -->

