for (var site in $tfs.SocialFollow.Following) { if (site == $tfs.CurrentSite.Key) { for (var social in $tfs.SocialFollow.Following[site]) { var timeStamp = $tfs.SocialFollow.Following[site][social]; timeStamp = timeStamp.replace('/Date(', '').replace(')/', ''); timeStamp = new Date(Date(timeStamp)).getTime(); if (now - timeStamp > thirtyDaysElapsed) { continue; } showTwitter &= social != 1; showFacebook &= social != 2; } } } if (showTwitter || showFacebook) { for (var site in $tfs.SocialFollow.OptedOut) { if (site == $tfs.CurrentSite.Key) { for (var social in $tfs.SocialFollow.OptedOut[site]) { var timeStamp = $tfs.SocialFollow.OptedOut[site][social]; timeStamp = timeStamp.replace('/Date(', '').replace(')/', ''); timeStamp = new Date(Date(timeStamp)).getTime(); if (now - timeStamp > fourteenDaysElapsed) { continue; } showTwitter &= social != 1; showFacebook &= social != 2; } } } } if ($(location).attr('pathname').toLowerCase() === '/socialfollow/landing') { showTwitter = false; showFacebook = false; } $tfs.SocialFollow.ShowTwitter = showTwitter; $tfs.SocialFollow.ShowFacebook = showFacebook; }; // Adblock check
