}); }catch(e){ //may want to output an error message to the console
} } oas_tag.query = (oas_tag.query != "" ? oas_tag.query + '&' : '') + 'status=' + $tfs.User.Status + '&site=' + $tfs.User.Site.Alias + '&age=' + $tfs.User.AgeRange + '&key=' + (window['model'] == undefined ? 'list' : window['model']) + '&NewsLetter=' + $tfs.Guest.Newsletter + (typeof document.referrer !== 'undefined' ? ('&refdn=' + referralDomain) : '') + ($('[data-tags]').length ? ('&keywords=' + keywords) : '') + ($('[data-campaign]').length ? ('&campaign=' + campaign) : ''); }; var default_sizes = function() { var rotators = []; if ($('[data-template="responsive"]').length) { var screenSize = Math.max(document.documentElement.clientWidth, window.innerWidth || 0) > 768 ? 'large' : 'small'; } $('.oas' + (screenSize ? ('.' + screenSize) : '')).each(function(i, ad) { if (ad.getAttribute('id') != "") { var id = ad.getAttribute('id').replace('oas_', ''); var width = ad.getAttribute('data-width'); var height = ad.getAttribute('data-height'); var expand = ad.getAttribute('data-expand'); var status = ad.getAttribute('data-status'); if ((status != null && status == $tfs.User.Status) || status == null) { if ($(this).closest('.adcontainer').length) { $(this).closest('.adcontainer').show(); } if (expand != null && expand !== undefined && expand !== 'undefined' && Math.max(document.documentElement.clientWidth, window.innerWidth || 0) > 728) { id = expand; width = 728; height = 90; ad.setAttribute('id', 'oas_' + id); ad.setAttribute('data-cb-ad-id', 'oas_' + id); ad.setAttribute('data-width', width); ad.setAttribute('data-height', height); ad.setAttribute('class', 'oas ad728_blk expand'); ad.removeAttribute('data-expand'); $('body').addClass('ad-expanded'); } if (id in sbi_trinity && Object.keys(sbi_trinity[id]).length > 0) { oas_tag.definePos(id, sbi_trinity[id].sbi_size.split('x')); } else { oas_tag.definePos(id, [width, height]); } } else if (status != null && status != $tfs.User.Status) { $(this).closest('.adcontainer').remove(); } } }); if (rotators.length > 0) { window.setInterval(function() { oas_tag.reloadAds(rotators); }, 1000 * 20); } }; window.debounce = function(func, wait) { var timeout; return function() { var context = this, args = arguments; var later = function() { timeout = null; }; var callNow = !timeout; clearTimeout(timeout); timeout = setTimeout(later, wait); if (callNow) func.apply(context, args); }; }; window.myReloadAds = debounce(function(ad_units) { window.sbi_trinity = {}; oas_tag.query = ""; load_trinity(function(ad_units) { set_query(); oas_tag.reloadAds(ad_units); }); }, 1500); load_trinity(function() { window.oas_tag = {}; oas_tag.url = 'oas.247sports.com'; //Define OAS URL
oas_tag.allowSizeOverride = true; oas_tag.sizes = default_sizes; updateNewsletter(); updateSocialFollow(); set_query(); (function() { oas_tag.version = '1'; var oas = document.createElement('script'), protocol = 'https:' == document.location.protocol ? 'https://' : 'http://',
node = document.getElementsByTagName('script')[0]; oas.type = 'text/javascript'; oas.async = true; oas.src = protocol + oas_tag.url + '/om/' + oas_tag.version + '.js'; node.parentNode.insertBefore(oas, node); })(); }); </script> </body> </html> 