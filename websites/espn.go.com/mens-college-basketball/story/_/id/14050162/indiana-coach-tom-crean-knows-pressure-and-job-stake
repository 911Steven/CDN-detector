		function loadDefer(){
			if( !deferLoaded ){
				var deferScripts = ['http://a.espncdn.com/redesign/0.277.10/js/espn-defer.js', 'http://a.espncdn.com/redesign/0.277.10/js/espn-defer-low.js'];
