<div class="question-summary narrow"
     id="question-summary-33234139"
     
     
     >
    <div onclick="window.location.href='/questions/33234139/why-doesnt-this-memory-eater-really-eat-memory'" class="cp">
        <div class="votes">
            <div class="mini-counts"><span title="126 votes">126</span></div>
            <div>votes</div>
        </div>
        <div class="status answered-accepted" title="one of the answers was accepted as the correct answer">
            <div class="mini-counts"><span title="4 answers">4</span></div>
            <div>answers</div>
