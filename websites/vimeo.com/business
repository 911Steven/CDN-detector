 function stopVideo() {
 intro_card.classList.remove('playing');
 PlayerManager.getPlayer(promo_player).pause();
 }

 function registerBigScreen() {
 try {
 BigScreen.on('exit', stopVideo);
 }
 catch(x) {
 console.warn(x);

 setTimeout(registerBigScreen, 1000);
 }
 }

