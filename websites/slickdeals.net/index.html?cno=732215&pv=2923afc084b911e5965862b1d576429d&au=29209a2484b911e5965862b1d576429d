    <script>
//Gift guide scroller 
if (typeof (giftGuideEnable) != 'undefined') { 
		 
	
	$(document).ready(function() {
		fixFilters();
	});

	// Fix filters	
	var fixFilters = function() {
		var mn = $(".ggtabs");
		mns = "main-nav-scrolled";
		hdr = $('.ggbanner').height();

		$(window).scroll(function() {
			if ($(this).scrollTop() > hdr) {
				mn.addClass(mns);
			} else {
				mn.removeClass(mns);
			}
		});
		

		$("a[href='#top']").click(function() {
			$("html, body").animate({
				scrollTop : 0
			}, "slow");
			return false;
		});

	}// Fix filters

}
</script>


















 

























 

























 













