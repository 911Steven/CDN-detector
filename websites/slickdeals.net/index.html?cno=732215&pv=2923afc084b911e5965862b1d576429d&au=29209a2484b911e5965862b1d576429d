/**
 *   Find in Store Filter
 */
 $(document).ready(function(){
	 $(document).on('click','#levi-store',function(){
		 if($(this).is(':checked')){
			 $('#FISResulttable').find('.visibleLevi').show();
		 }else{
			 $('#FISResulttable').find('.visibleLevi').hide();
		 }
		 updateFisRecordChk();
	 });
	 $(document).on('click','#Macy',function(){
		 if($(this).is(':checked')){
			 $('#FISResulttable').find('.visibleMacy').show();
		 }else{
			 $('#FISResulttable').find('.visibleMacy').hide();
		 }
		 updateFisRecordChk();
	 });
 });
 
 function updateFisRecordChk(){
	 if($('#FISResulttable').length){
		 $('.filter_result').show();
		 var numOfVisibleRows = $('#FISResulttable > tbody > tr').filter(function() {
			  return $(this).css('display') !== 'none';
		 }).length;
		 $('.filter_result .count').html(numOfVisibleRows);
		 /**
		  *  If there is no check box is checked, then showing empty/no result message
		  */
		 if(numOfVisibleRows == 0){
			 $('#FISResulttable').hide();
			 $('.searchresultContainer').show();
		 }else{
			 $('#FISResulttable').show();
			 $('.searchresultContainer').hide();
		 }
	 }else{
		 $('.filter_result').hide();
		 $('.filter_result .count').empty();
	 }
  }
</script>

 



