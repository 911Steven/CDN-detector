	});
   	
	
	$.urlParam = function(name,strHref){
	    var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(strHref);
	    if (results==null){
	       return null;
	    }
	    else{
	       return results[1] || 0;
	    }
	}

	$(document).on("click", "a[href*='ab=']" , function (i, el) {
	         var hrefStr = $(this).attr("href");
			 if(undefined != hrefStr &&  hrefStr != ""){
			 var abParam=$.urlParam('ab',hrefStr);
			  if(undefined != abParam &&  abParam != ""){
				$(window).trigger("elementInteraction", {type: "click", elementType: "internal campaign link", campaignID: abParam});
			 }
			 }
			  
	});
		 
	$(document).on("click", "area[href*='ab=']" , function (i, el) {
			var hrefStr = $(this).attr("href");
			if(undefined != hrefStr &&  hrefStr != ""){
			var abParam=$.urlParam('ab',hrefStr);
			if(undefined != abParam &&  abParam != ""){
				$(window).trigger("elementInteraction", {type: "click", elementType: "internal campaign link", campaignID: abParam});
			 }
			 }
	});
	
	</script>
