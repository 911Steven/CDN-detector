    //this == the link that was clicked
    var linkId = $(this).attr("id");
    if(undefined != linkName &&  linkName != ""){
		 $(window).trigger("elementInteraction", {type: "click", elementType: "navigation link", linkID: linkId});
    }
});

$(document).on('change', '.card-type-options input', function(e) {
		var paymentType = $(this).attr('data-payment-type');
		var id = $(this).attr('id');
		if(paymentType === 'creditcard') {
			$(window).trigger("elementInteraction", {type: "onchange", elementType: "radio button", selectedOption: paymentType+':'+ (id.split("-"))[1]});
		}
		else {
			$(window).trigger("elementInteraction", {type: "onchange", elementType: "radio button", selectedOption: 'banktransfer:'+ (id.split("-"))[1]});
		}
});
