		$('.mini-cart-container').live({
		    'mouseover':function(){
		    	if($(this).find(".mini-cart .qty").text()!=0){
		    		$("#mini-cart-details.hidden-cart").show();
		    	}
		    	else{
		    		$("#mini-cart-details.hidden-cart").hide();
		    	}
		    	
		    },'mouseout':function(){
		    	$("#mini-cart-details.hidden-cart").hide();
		    }
		});
	});	
 
