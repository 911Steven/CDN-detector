    <script type="text/javascript">

$(document).on("click", ".latest-deals a", function() {
    //this == the link that was clicked
    var linkId = $(this).attr("href");

    if(undefined != linkId && linkId != ''){
    	var res = linkId.split("/");
    	if(res.length == 5){
    	    $(window).trigger("elementInteraction", {type: "click", elementType: "internal campaign link", campaignID: res[4]});
    	}
    }

});

$(document).on("click", ".footer-service a", function() {
    //this == the link that was clicked
    var linkId = $(this).attr("id");
    if(undefined != linkId &&  linkId != ""){
		 $(window).trigger("elementInteraction", {type: "click", elementType: "navigation link", linkID: linkId});

    }

});
