<div class="content wysiwyg"><script type="text/javascript">
	$(function(){
		// Reg Resize
		var reg_obj = $("#main-container sup.resized");
		$.each( reg_obj, function() {

			var font_size = $(this).css( "fontSize" );
			font_size_old = font_size.replace( "px" , "" );
			font_size_new = Math.round( font_size_old * 0.3 );

			var top = Math.round( font_size_new * 1.62 );
			var position = "relative";
			var decoration = "none";

			$(this).css({
				fontSize: font_size_new + 'px' ,
				position: position,
				top: '-' + top + 'px',
				textDecoration: decoration
			});

		});
	});
</script></div>
</div>

