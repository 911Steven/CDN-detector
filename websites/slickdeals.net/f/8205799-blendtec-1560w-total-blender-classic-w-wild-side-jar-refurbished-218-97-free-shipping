      if (pageData.categories && pageData.categories.length > 0)
      {
        ctg = [];
        for (var i = 0; i < pageData.categories.length; i++)
        {
          ctg.push("ctg"+(i+1)+"="+encodeURIComponent(pageData.categories[i]));
        }
        ctg = ctg.join("&");
      }
      var xl8_script = document.createElement("script");
      xl8_script.src = "http://loadus.exelator.com/load/?p=515&g=002&c=20210&"+ctg+"&ag=&gd=&j=d";
      xl8_script.type = "text/javascript";
      xl8_script.async = true;
      var s = document.getElementsByTagName('script')[0];
      s.parentNode.insertBefore(xl8_script, s);
    });
  </script>

  
      
