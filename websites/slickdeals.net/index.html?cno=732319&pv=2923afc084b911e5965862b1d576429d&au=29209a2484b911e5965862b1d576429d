							 <script type="text/javascript">      
$(document).ready(function() {

	var objMobileLink = $("#mobileview_link");

	pageURL = document.URL;
	if(pageURL.indexOf("http://m.") != -1 || pageURL.indexOf("https://m.") != -
1){
		objMobileLink.hide();
	}
	else{
		
		var mobileViewURL = "http://m.bonton.com/";

		if (typeof divisionDomain !== 'undefined') {
			if(divisionDomain != null && objMobileLink){
			
				mobileViewURL = "http://m."+region+divisionDomain
+".com/";

				objMobileLink.attr("href",mobileViewURL);
			}
		}
	}

});
</script>

