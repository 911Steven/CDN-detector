<script type="text/javascript">
	
	$(document).ready(function(){		
		//$("#productRecZone").append("<div id='owl-reczone'></div>");
		$('div[id*="productRecZone"]').append("<div id='owl-reczone'></div>");	
		
		var pageURL = document.location.pathname;
			
		if(pageURL == "/checkout/"){
			//$('div[id*="productRecZone"]').insertAfter($("#shoppingBagPage")).css("margin-top","10px");
			$("#mainarea").append($('div[id*="productRecZone"]').css("margin-top","10px"));
		}
		
		$(".featuredProduct").each(function(index, element) {
			anchorElement = $(this).find("a.productLink");
			curImageId = anchorElement.attr("href").substr(anchorElement.attr("href").indexOf(".html")-6, 6);
            if($("#productPartVar").val() != curImageId){
				$(this).appendTo($("#owl-reczone"));
			}
			else{
				$(this).remove();
			}
        });
		
		$("#owl-reczone").owlCarousel({
			items : 7					
		});
		
	});
</script>
