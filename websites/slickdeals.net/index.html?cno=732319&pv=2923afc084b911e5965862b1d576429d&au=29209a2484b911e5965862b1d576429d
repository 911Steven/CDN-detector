if (1 == 0){  // turn this off


$(document).ready(function() {
	if (typeof(orderData) !== 'undefined'){  // Check if Adlucent data is defined
	// debugging...
	if (typeof console == "object") { 
		console.log("****************************************************************************"); 
		console.log("order data found"); 
		console.log("order number: " + orderKey); 
		console.log("****************************************************************************"); 
	}

		var GTMOrderTotal = "";
		var GTMOrderSubTotal = "";

		if (isMobile === true){
			// debugging...
			if (typeof console == "object") { 
				console.log("****************************************************************************"); 
				console.log("isMobile"); 
				console.log("****************************************************************************"); 
			}
			GTMOrderTotal = $(".orderTotals").find('span:nth-child(2)').text().trim();
			GTMOrderSubTotal = $("#orderSummary").find('span:eq(1)').text().trim();
		}else{
			GTMOrderTotal = $(".totalAmount").text().trim();
			GTMOrderSubTotal = $("#orderTotals").find('td:eq(3)').text().trim();
		}

		// debugging...
		if (typeof console == "object") { 
			console.log("****************************************************************************"); 
			console.log("GTMOrderSubTotal: " + GTMOrderSubTotal); 
			console.log("****************************************************************************"); 
		}

		dataLayer.push({
			'transactionId': orderKey,
			'transactionGrandTotal': GTMOrderTotal,  // with tax + ship
			'transactionTotal': GTMOrderSubTotal,  // no tax or ship
			'transactionSubTotal': GTMOrderSubTotal,  // no tax or ship
			'transactionShipping': CI_Shipping,
			'transactionTax': CI_Tax //,
//			'visitorType': 'Guest'
		});
	}
});

} // end if 1=0

</script>

