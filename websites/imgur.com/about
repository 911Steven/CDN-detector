            $('#about1, #about2, #about3').click(function(){
                var $el = $(this);
                showPage($el.data('content'), $el);
                updatePushState($el);
            });
            
            function showPage(divName, $el) {
                $('.active').removeClass('active');
                $el.addClass('active');
                $('.page').addClass('nodisplay');
                $('.' + divName).removeClass('nodisplay');
            };
            
            function updatePushState($el) {
                window.history.pushState({}, '', '/' + $el.data('src'));
            }
            
            window.onpopstate = function(e) {
                var href = document.location.href;
                var content = href.substring(href.lastIndexOf('/')+1);
                showPage(content + '-content', $('[data-content="' + content + '-content"]'));
            };
        
    </script>

    

